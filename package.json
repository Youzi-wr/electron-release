{
  "name": "electron-updater-example",
  "version": "0.2.0",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "pack": "electron-builder --ia32 --x64 -w",
    "publish": "build --win -p always"
  },
  "devDependencies": {
    "electron": "3.1.11",
    "electron-builder": "^20.15.1"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^2.21.10"
  },
  "build": {
    "appId": "com.github.iffy.electronupdaterexample",
    "electronDownload": {
      "cache": "./zips"
    },
    "publish": {
      "provider": "generic",
      "url": "http://localhost:8000/test-release/"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "directories": {
      "output": "./outPut"
    }
  }
}
